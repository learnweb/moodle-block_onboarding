<div class="block-experiences-overview-experiences">
    <navigation>
        <a href="../index.php">
            {{#pix}}t/left, core,Back{{/pix}}{{# str }}backtohome, block_onboarding{{/ str }}
        </a>
        <div class="experiences-title-wrapper">
            {{#can_manage_experiences}}
                <a href="admin.php">
                    {{#pix}}i/settings, core, {{# str }}settings, block_onboarding{{/ str }}{{/pix}}
                    {{# str }}settings, block_onboarding{{/ str }}
                </a>
            {{/can_manage_experiences}}
        </div>
    </navigation>
    <filter>
        {{#form}}{{{form}}}{{/form}}
    </filter>
    <aside>
        <div class="block-experiences-overview-experiences-actions">
            <div class="title">{{# str }} myexperiencearea, block_onboarding {{/ str }}</div>
            {{^experience}}
                <a href="edit_experience.php">{{# str }}newexperience, block_onboarding{{/ str }}</a>
            {{/experience}}
            <br>
            <div class="title-2-light">{{#experience}}
                <a href="experience.php?experience_id={{{id}}}">
                    {{{name}}}
                    {{^published}}[{{# str }}draft, block_onboarding{{/ str }}]{{/published}}
                </a>
            {{/experience}}
            {{#experiences_with_categories}}{{#editable}}
                <a href="edit_experience.php?experience_id={{{id}}}">{{#pix}}t/editstring, core, {{# str }}edit_experience, block_onboarding{{/ str }}{{/pix}}</a><span onb-data-id="{{{id}}}" onb-data-context="exp-exp" class="delete-btn">{{#pix}}t/delete, core, {{# str }}edit_experience, block_onboarding{{/ str }}{{/pix}}</span>
            {{/editable}}{{/experiences_with_categories}}</div>
            <!--{{#categories_general}}<p class="block-experiences-overview-category-badge">{{{name}}}</p> {{/categories_general}}-->
        </div>
    </aside>
</div>
<br>
