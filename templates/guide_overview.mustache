<a href="../index.php">{{#pix}}t/left, core, {{# str }}back, block_onboarding{{/ str }}{{/pix}}{{# str }}backtohome, block_onboarding{{/ str }}</a>
<br>
<div class="block-onboarding-steps-title-wrapper">
    <span class="block-onboarding-title">{{# str }}steps, block_onboarding{{/ str }}</span>
    <span class="block-onboarding-steps-title-button-wrapper">
        <span class="block-onboarding-steps-hide-btn">{{#pix}}i/show, core, {{# str }}hide_steps, block_onboarding{{/ str }}{{/pix}}{{# str }}hide, block_onboarding{{/ str }}</span>
        <span class="block-onboarding-steps-show-btn">{{#pix}}i/hide, core, {{# str }}show_steps, block_onboarding{{/ str }}{{/pix}}{{# str }}show, block_onboarding{{/ str }}</span>
        {{#can_manage_steps}}
            <a href="admin_steps.php">{{#pix}}i/settings, core, {{# str }}manage_steps, block_onboarding{{/ str }}{{/pix}}{{# str }}settings, block_onboarding{{/ str }}</a>
        {{/can_manage_steps}}
    </span>
</div>
<div class="block-onboarding-steps-container">
    <canvas id="block-onboarding-steps-achievement-confetti"></canvas>
    <div class="block-onboarding-steps-progress-bar">
        <div class="block-onboarding-steps-progress-bar-value"></div>
        <div class="block-onboarding-steps-progress-bar-fill"></div>
    </div>
    <div class="block-onboarding-steps-content-wrapper">
        <div class="block-onboarding-steps-title-wrapper">
            <span class="block-onboarding-steps-step-title"></span>
            <span class="block-onboarding-steps-step-completed">{{# str }}completed, block_onboarding{{/ str }}</span>
        </div>
        <div class="block-onboarding-steps-step-description"></div>
        <div class="block-onboarding-steps-button-wrapper">
            <button class="block-onboarding-steps-back-btn btn btn-primary" type=button>
                {{# str }}button_back, block_onboarding{{/ str }}
            </button>
            <button class="block-onboarding-steps-next-btn btn btn-primary" type=button>
                {{# str }}button_done, block_onboarding{{/ str }}
            </button>
            <button class="block-onboarding-steps-reset-btn btn btn-danger" type=button>
                {{# str }}button_reset, block_onboarding{{/ str }}
            </button>
        </div>
    </div>
</div>
<br>
<br>
<div class="block-onboarding-steps-title-wrapper">
    <div class="block-onboarding-title">{{# str }}wiki, block_onboarding{{/ str }}</div>
    {{#can_manage_wiki}}
        <a href="admin_wiki.php" class="admin-link">
            {{#pix}}i/settings, core, {{# str }}manage_wiki, block_onboarding{{/ str }}{{/pix}}
            {{# str }}settings, block_onboarding{{/ str }}
        </a>
    {{/can_manage_wiki}}
</div>
{{#categories_with_links}}
    <div class="block-onboarding-wiki-cat-container">
        <div class="block-onboarding-wiki-cat-title-wrapper">{{{name}}}</div>
        <div class="block-onboarding-wiki-cat-content-wrapper">
            <ul>
                {{#links}}
                    <li>
                        <a target="_blank" rel="noopener noreferrer" href="{{{url}}}">{{{name}}}</a>
                        <div class="block-onboarding-custom-tooltip">{{#pix}}i/info, core,{{/pix}}
                            <span class="custom-tooltiptext">{{{description}}}</span>
                        </div>
                    </li>
                {{/links}}
            </ul>
        </div>
    </div>
{{/categories_with_links}}